{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"custom": {
			"wporg-site-screenshot": {
				"border": {
					"color": "rgba(30, 30, 30, 0.1)",
					"width": "10px",
					"radius": "20px"
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-submit",
			"title": "Submission Page",
			"postTypes": [ "page" ]
		}
	]
}
